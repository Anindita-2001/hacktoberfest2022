 class Recursion4
{
	public static void printFibo(int x,int y,int n)
	{
		if(n==0)
		{	
			return ;
		}
		int z=x+y;
		System.out.println(z);
		printFibo(y,z,n-1);
	}
	public static void main(String args[])
	{
		int x=0,y=1;
		System.out.println(x);
		System.out.println(y);
		int n=5;
		printFibo(x,y,n-2);
	
		
	}
}	
